        <link rel="stylesheet" href="{{ asset('assets/css/styleLinearGradien.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('assets/jquery/slick/slick-theme-perso.css') }}"/>
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
        
